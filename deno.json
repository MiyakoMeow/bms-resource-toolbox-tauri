{
  "imports": {
    "@eslint/compat": "npm:@eslint/compat@^2.0.0",
    "@eslint/core": "npm:@eslint/core@^1.0.0",
    "@eslint/js": "npm:@eslint/js@^9.39.2",
    "@sveltejs/adapter-static": "npm:@sveltejs/adapter-static@^3.0.6",
    "@sveltejs/kit": "npm:@sveltejs/kit@^2.9.0",
    "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^5.0.0",
    "@tailwindcss/node": "npm:@tailwindcss/node@^4.1.18",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.0",
    "@tauri-apps/api": "npm:@tauri-apps/api@^2",
    "@tauri-apps/cli": "npm:@tauri-apps/cli@^2",
    "@tauri-apps/plugin-dialog": "npm:@tauri-apps/plugin-dialog@^2",
    "@tauri-apps/plugin-fs": "npm:@tauri-apps/plugin-fs@^2.4.5",
    "@tauri-apps/plugin-opener": "npm:@tauri-apps/plugin-opener@^2",
    "@tauri-apps/plugin-shell": "npm:@tauri-apps/plugin-shell@^2.3.4",
    "@types/eslint-config-prettier": "npm:@types/eslint-config-prettier@^6.11.3",
    "@types/node": "npm:@types/node@^25.0.3",
    "@typescript-eslint/parser": "npm:@typescript-eslint/parser@^7.18.0",
    "autoprefixer": "npm:autoprefixer@^10.4.23",
    "clsx": "npm:clsx@^2.1.1",
    "eslint": "npm:eslint@^9.39.2",
    "eslint-config-prettier": "npm:eslint-config-prettier@^9.1.2",
    "eslint-plugin-better-tailwindcss": "npm:eslint-plugin-better-tailwindcss@^4.0.1",
    "eslint-plugin-svelte": "npm:eslint-plugin-svelte@^2.46.1",
    "globals": "npm:globals@^17.0.0",
    "prettier": "npm:prettier@^3.7.4",
    "prettier-plugin-svelte": "npm:prettier-plugin-svelte@^3.4.1",
    "prettier-plugin-tailwindcss": "npm:prettier-plugin-tailwindcss@^0.7.2",
    "svelte": "npm:svelte@^5.0.0",
    "svelte-check": "npm:svelte-check@^4.0.0",
    "synckit": "npm:synckit@^0.11.11",
    "tailwindcss": "npm:tailwindcss@^4.0.0",
    "typescript": "npm:typescript@~5.6.2",
    "typescript-eslint": "npm:typescript-eslint@^8.52.0",
    "vite": "npm:vite@^6.0.3",
    "xlsx": "npm:xlsx@^0.18.5"
  },
  "tasks": {
    "dev": "deno run -A npm:vite dev",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "check": "deno run -A npm:svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "deno run -A npm:svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "deno run -A npm:eslint",
    "lint:fix": "deno run -A npm:eslint --fix",
    "format": "deno run -A npm:prettier --check .",
    "format:fix": "deno run -A npm:prettier --write .",
    "tauri": "deno run -A npm:tauri"
  },
  "exclude": ["target/", "build/", ".svelte-kit/"],
  "nodeModulesDir": "auto",
  "lock": true
}
